<h1>物件一覧</h1>
<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>
  <tr>
  <% @houses.each do |house| %>
    <td><%= house.title %></td>
    <td><%= house.rent_fee %></td>
    <td><%= house.address %></td>
    <td><%= house.age %>年</td>
    <td><%= house.remark %></td>
    <td>詳細</td>
    <td>編集</td>
    <td>削除</td>
　</tr>
 <% end %>
</table>

<p><%= link_to '物件を登録する', new_house_path %><p>
